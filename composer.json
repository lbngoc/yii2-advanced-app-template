{
    "name": "lbngoc/yii2-advanced-app-template",
    "description": "Yii2 Advanced Template and API",
    "keywords": ["yii2", "rest", "application template", "api"],
    "homepage": "https://github.com/lbngoc/yii2-advanced-app-template",
    "type": "project",
    "license": "BSD-4-Clause",
    "authors": [
        {
            "name": "Ngoc Luong-Bao",
            "email": "baongock32@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/lbngoc/yii2-advanced-app-template/issues",
        "source": "https://github.com/lbngoc/yii2-advanced-app-template"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "mdmsoft/yii2-admin": "~2.0",
        "fxp/composer-asset-plugin": "~1.0",
        "dmstr/yii2-adminlte-asset": "2.*",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-detail-view": "@dev",
        "amnah/yii2-user": "3.*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "mdmsoft/yii2-gii": "~1.0",
        "sintret/yii2-gii-adminlte": "dev-master"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
